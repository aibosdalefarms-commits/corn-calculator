  "devDependencies": {
    "canvas": "^2.11.2",
    "http-server": "^14.1.1"
  },
  "optionalDependencies": {
    "firebase-tools": "^13.0.0",
    "netlify-cli": "^17.0.0",
    "vercel": "^33.0.0"
  }
}
